# update homebrew
alias brewu='brew update && brew upgrade && brew doctor'

# fix homebrew-cask importing errors
alias brewfix='ls -l /usr/local/Library/Formula | grep phinze-cask | awk "{print $9}" | for evil_symlink in $(cat -); do rm -v /usr/local/Library/Formula/$evil_symlink; done'

# cd to default Code directory
alias cdc='cd ~/Dropbox/Work/personal/code'

# cd to default Processing directory
alias cdp='cd ~/Dropbox/Work/personal/processing'

# get high-quality version from youtube videos
alias movhigh='movgrab -t youtube -f mp4:1280'

# delete .DS_Store files
alias dsd='find . -iname ".DS_Store" -delete'

# delete empty files
alias zerod='find . -type f -empty -delete'
