# update homebrew
alias brewu='brew update && brew upgrade && brew doctor'

# chrome, clear cache
alias chromeclear='mv ~/Library/Caches/Google/Chrome/Default/* ~/.Trash/'

# cd to default Code directory
alias cdcode='cd ~/Dropbox/Work/personal/code'

# get high-quality version from youtube videos
alias movhigh='movgrab -f mp4:1280'

# delete .DS_Store files
alias dsdelete='find . -iname ".DS_Store" -delete'

# delete empty files
alias zerodelete='find . -type f -empty -delete'
